{
  "name": "figma-variables-gh-actions",
  "packageManager": "yarn@4.7.0",
  "scripts": {
    "build": "node ./src/build.mjs",
    "tokens:import:colour": "npx tsx ./src/figma-api/syncFigmaToTokens.ts",
    "tokens:import:palette": "npx tsx ./src/figma-api/syncFigmaToTokens.ts --isPalette=true",
    "tokens:import": "npx tokens:import:palette && npx tokens:import:colour"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "dotenv": "^16.4.7",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/yargs": "^17.0.33",
    "style-dictionary": "^4.3.3",
    "tsx": "^4.19.3",
    "yargs": "^17.7.2"
  }
}
